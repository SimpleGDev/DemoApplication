<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.demoapplication" >

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".fab.FabActivity" />
        <activity android:name=".drawer.NavigationDrawerActivity" />
        <activity android:name=".viewpager.ViewPagerActivity" />
        <activity android:name=".CardViewActivity" />
        <activity android:name=".notification.NotificationActivity"/>

        <service
            android:name=".notification.DemoIntentService"
            android:exported="false" >
        </service>
    </application>

</manifest>
